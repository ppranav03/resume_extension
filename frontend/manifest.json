{
  "manifest_version": 3,
  "name": "Referral Finder",
  "version": "1.0",
  "description": "Find contacts for a job you are interested in!",
  "permissions": ["tabs", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "popup.js"
  }
}
  